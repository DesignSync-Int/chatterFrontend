{
  "name": "chatter",
  "private": true,
  "version": "1.2.3",
  "type": "module",
  "author": "Sachin",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build && cp public/_redirects dist/_redirects",
    "preview": "vite preview",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test": "npm run test:e2e",
    "lint": "eslint . --ext .ts,.tsx --max-warnings=100",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit -p tsconfig.app.json -p tsconfig.node.json",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "quality": "npm run lint && npm run typecheck",
    "version:patch": "npm version patch --no-git-tag-version",
    "version:minor": "npm version minor --no-git-tag-version",
    "version:major": "npm version major --no-git-tag-version",
    "update-version": "node scripts/update-version.js",
    "prepare": "husky"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.10",
    "@tanstack/react-query": "^5.79.0",
    "@types/socket.io-client": "^1.4.36",
    "axios": "^1.10.0",
    "bad-words": "^4.0.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "leo-profanity": "^1.8.0",
    "lucide-react": "^0.511.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hot-toast": "^2.5.2",
    "react-router-dom": "^7.6.3",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.10",
    "zod": "^4.0.5",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.25.0",
    "@types/axios": "^0.9.36",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "cypress": "^14.5.1",
    "cypress-file-upload": "^5.0.8",
    "eslint": "^9.30.1",
    "eslint-plugin-cypress": "^5.1.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-testing-library": "^7.6.1",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  }
}
